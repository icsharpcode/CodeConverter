{
    "name": "codeconverter",
    "version": "0.1.0",
    "private": true,
    "eslintConfig": {
        "extends": "react-app"
    },
    "scripts": {
        "prestart": "node aspnetcore-https && node aspnetcore-react",
        "start": "rimraf ./build && react-scripts start",
        "build": "react-scripts build",
        "test": "cross-env CI=true react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "lint": "eslint ./src/"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-react",
                {
                    "runtime": "automatic"
                }
            ]
        ]
    },
    "dependencies": {
        "@monaco-editor/react": "^4.7.0",
        "axios": "^1.13.4",
        "bootstrap": "^5.3.8",
        "monaco-editor": "^0.55.1",
        "prop-types": "^15.8.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-router-bootstrap": "^0.26.3",
        "react-router-dom": "^7.13.0",
        "reactstrap": "^9.2.3"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/react": "^19.2.13",
        "@types/react-dom": "^19.2.3",
        "ajv": "^8.17.1",
        "cross-env": "^10.1.0",
        "eslint-config-react-app": "^7.0.1",
        "http-proxy-middleware": "^3.0.5",
        "jquery": "^4.0.0",
        "popper.js": "^1.16.1",
        "react-scripts": "^5.0.1",
        "rimraf": "^6.1.2",
        "typescript": "^5.9.3"
    }
}
