	<script id="GTMScript" type="text/plain" class="optanon-category-1">
		(function (w, d, s, l, i) {
			w[l] = w[l] || []; w[l].push({
				'gtm.start':
				new Date().getTime(), event: 'gtm.js'
			}); var f = d.getElementsByTagName(s)[0],
				j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
					'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
		})(window, document, 'script', 'dataLayer', 'GTM-6X92');</script>
	<!-- End Google Tag Manager -->


	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>Code Converter C# to VB and VB to C# – Telerik</title>
	@RenderSection("head", required: false)

	<meta name="keywords" content="code converter, csharp to vb, vb to csharp, c#, vb.net" />
	<meta name="description" content="Telerik Code Converter by Progress is free online code converter from C# to VB and from VB to C#. No registration required. Check it out." />
	<meta name="robots" content="index,follow">
	<link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />

	<link rel="stylesheet" href="https://d6vtbcy3ong79.cloudfront.net/fonts/1.1.5/css/metric.min.css" />
	<link rel="stylesheet" href="~/Content/bootstrap.min.css" />
	<link rel="stylesheet" href="~/Content/site.css" />
</head>
<body class="d-flex flex-column">
	<!-- Google Tag Manager (noscript) -->
	<noscript>
		<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-6X92"
				height="0" width="0" style="display:none;visibility:hidden"></iframe>
	</noscript>
	<!-- End Google Tag Manager (noscript) -->

	<div class="navbar navbar-inverse">
		<div class="navbar-header">
			<a href="~/" class="navbar-brand"><img src="~/Images/logo.svg" alt="Code Converter Logo" /></a>
		</div>
	</div>
	<div class="body-content p-2 flex-100">
		@RenderBody()
		@{
			var versionInfo = HttpContext.Current.Cache["AssemblyVersionInformation"];
			if (null == versionInfo)
			{
				Assembly assembly = Assembly.GetAssembly(typeof(CodeConverter));
				FileVersionInfo fvi = FileVersionInfo.GetVersionInfo(assembly.Location);

				versionInfo = fvi.FileVersion;
				HttpContext.Current.Cache["AssemblyVersionInformation"] = versionInfo;
			}
		}
	</div>
	<footer class="px-2 py-3">
		<div class="row mb-1">
			<div class="col col-xs-12">
				<a href="https://www.telerik.com">
					<img class="logo-footer" src="~/Images/logo-telerik.svg" alt="Progress Telerik" />
